{
  "name": "midwest-map",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "next build",
    "start": "next start",
    "lint": "biome check src && next lint",
    "lint:fix": "biome check --write src && next lint --fix",
    "format": "biome format --write src",
    "compile-importer": "tsc -p tsconfig-import-shapefiles.json",
    "import-shapefiles": "node out-scripts/src/script/ImportShapefiles.js clusters-2025.shp",
    "import-counties": "node out-scripts/src/script/ImportCounties.js",
    "merge-timeline": "node out-scripts/src/script/MergeAdvancementData.js",
    "map-counties": "node out-scripts/src/script/MapCountiesToClusters.js",
    "prepare-data": "pnpm compile-importer && pnpm import-shapefiles && pnpm import-counties && pnpm merge-timeline && pnpm map-counties"
  },
  "dependencies": {
    "@loaders.gl/core": "^4.3.4",
    "@loaders.gl/loader-utils": "^4.3.4",
    "@loaders.gl/shapefile": "^4.3.4",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.19",
    "@turf/boolean-point-in-polygon": "^7.3.2",
    "@turf/helpers": "^7.3.2",
    "@turf/point-on-feature": "^7.3.2",
    "@types/geojson": "^7946.0.16",
    "@types/mapbox-gl": "^3.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fast-equals": "^6.0.0",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^3.18.0",
    "next": "^15.5.9",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-map-gl": "^8.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-extras": "^0.16.1",
    "zod": "^4.3.5",
    "zod-geojson": "^1.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "csv-parse": "^6.1.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^15.4.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "type-fest": "^5.4.1",
    "typescript": "^5.9.3"
  }
}
